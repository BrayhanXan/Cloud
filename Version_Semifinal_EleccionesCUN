//Pedir al usuario la cantidad de votos a simular
n = Leer("Ingrese el número de estudiantes que van a votar (máximo 20): ")

Si n > 20 Entonces

para i = 1, i=n, i++
    v=Leer("Ingrese el nombre del representante")
    si v="Hollman"
        n1+=1 
    Si v ="Fernando"
        n2+=1
    Si v="Daniel"
        n3+=1
    Si v="Brayhan"
        n4+=1
    Si v = "Joel"
        n5+=1
    No
        Imprimir "ingrese un representate válido"
        
n6 = [n1, n2, n3, n4, n5]
Si (n1,n2,n3,n4,n5 > 20)

//Mostrar el representante con mas votos

Ordenado = ""

si n1 < n2 entonces        
    temp = n1
    n1 = n2
    n2 = temp
fin si

si n2 < n3 entonces
    temp = n2
    n2 = n3
